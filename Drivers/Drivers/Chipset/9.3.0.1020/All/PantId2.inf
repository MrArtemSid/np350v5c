; ****************************************************************************
; ****************************************************************************
; **    Filename:  PantID2.INF
; **    Abstract:  Windows* 2K, XP, and 2003 Server, Vista INF File for 
; **               Intel(R) IDE device (IntelDrver)
; **    Last Update: August 26, 2011 (Version 9.3.0 Build 1011)
; ****************************************************************************
; ****************************************************************************

[version]
Signature="$WINDOWS NT$"
Class=hdc
ClassGuid={4D36E96A-E325-11CE-BFC1-08002BE10318}
Provider=%INTEL%
CatalogFile=Pantid2.cat
DriverVer=08/26/2011, 9.3.0.1011

[SourceDisksNames] 
[SourceDisksFiles] 

[PackageInfo]
Name=Chipset
Condition=InstalledDriverSection(intelide_Inst)

[ControlFlags]
ExcludeFromSelect=*

[Manufacturer]
%INTEL%=INTEL_HDC,NTamd64

[INTEL_HDC]
;IDE
%PCI\VEN_8086&DEV_1E00.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E00
%PCI\VEN_8086&DEV_1E01.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E01
%PCI\VEN_8086&DEV_1E08.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E08
%PCI\VEN_8086&DEV_1E09.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E09

[INTEL_HDC.NTamd64]
;IDE
%PCI\VEN_8086&DEV_1E00.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E00
%PCI\VEN_8086&DEV_1E01.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E01
%PCI\VEN_8086&DEV_1E08.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E08
%PCI\VEN_8086&DEV_1E09.DeviceDesc%=intelide_Inst,PCI\VEN_8086&DEV_1E09


;**********************************************************
; Intel PCI IDE Controller (DUAL_GOOD_IDE)

[intelide_Inst]
Include=mshdc.inf
Needs=intelide_Inst
AddReg = intelChannelEnableCheck_AddReg

[intelChannelEnableCheck_AddReg]
HKR,,MasterOnMask,%REG_DWORD%,0x00000080
HKR,,MasterOnConfigOffset,%REG_DWORD%,0x00000041
HKR,,SlaveOnMask,%REG_DWORD%,0x00000080
HKR,,SlaveOnConfigOffset,%REG_DWORD%,0x00000043

[intelide_Inst.Services]
Include=mshdc.inf
Needs=intelide_Inst.Services

[intelide_Inst.PosDup]
*PNP0600

[Strings]
INTEL="Intel"
;IDE
PCI\VEN_8086&DEV_1E00.DeviceDesc="Intel(R) 7 Series/C216 Chipset Family 4 port Serial ATA Storage Controller - 1E00"
PCI\VEN_8086&DEV_1E01.DeviceDesc="Intel(R) 7 Series/C216 Chipset Family 4 port Serial ATA Storage Controller - 1E01"
PCI\VEN_8086&DEV_1E08.DeviceDesc="Intel(R) 7 Series/C216 Chipset Family 2 port Serial ATA Storage Controller - 1E08"
PCI\VEN_8086&DEV_1E09.DeviceDesc="Intel(R) 7 Series/C216 Chipset Family 2 port Serial ATA Storage Controller - 1E09"


;*******************************************
;Handy macro substitutions (non-localizable)
REG_DWORD              = 0x00010001

